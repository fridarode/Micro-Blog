    <h2>Create new User</h2>
    <form class="" action="/users/new" method="post">
        <input type="text" name= "[post]fname" placeholder="First Name">
        <input type="text" name= "[post]lname" placeholder="Last Name">
        <input type="text" name= "[post]password" placeholder="Password">
        <input type="text" name= "[post]age" placeholder="Age">

        <input type="submit" name="" value="Create">
    </form>

    <h2>Sign in User</h2>
    <form class="" action="/sign-in" method="post">
        <input type="text" name="fname" placeholder="First Name">
        <input type="text" name="password" placeholder="Password">
        <input type="submit" name="" value="Sign In">
    </form>
  </div>
</div>
